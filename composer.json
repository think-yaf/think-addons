{
    "name": "thinkyaf/think-addons",
    "description": "ThinkPHP6 think-addons",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "version": "1.0",
    "authors": [
        {
            "name": "Tan Minghe",
            "email": "tanminghe@126.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        },
        "files": [
            "src/common.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\addons\\Service"
            ],
            "config": {
                "addons": "src/config.php"
            }
        }
    }
}